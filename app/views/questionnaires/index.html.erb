<div class="container">
	<div class="row">
		<div class="col-md-4 col-md-offset-2">
			<div class="panel panel-info">
				<div class="panel-heading"><h2>Questionnaires List</h2></div>
				<div class="panel-body">
					<% if Questionnaire.count==0 %>
						<h4>No questionnaires yet, would you like to create a new one?</h4>
					<% else  %>
						<% @questionnaires.each do |x| %>

						<% end %>
					<% end %>
					<div class="form">
						<h4>Create a new questionnaire with
						<% form_tag(new_questionnaire_path) %>
						<%= text_field_tag :number_of_questions  %>
						<%= submit_tag 'Go' %>
						</h4>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>