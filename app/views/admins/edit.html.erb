
<% @page_title = 'Admin - Edit'%>
<div class="container">
	<div class="row">
		<div class="col-md-6 col-md-offset-2">
			<div class="panel panel-warning">
				<div class="panel-heading"><h3>Edit admin details </h3></div>
				<div class="panel-body">
					<%= form_for @admin , url: {action: 'update'} do |f|%>
						<%= render partial: 'admins/form', locals: {f: f }%>
						<%= f.submit %>
					<% end %>
				</div>
			</div>
		</div>
	</div>
</div>