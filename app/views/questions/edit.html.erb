<h1>Edit a question</h1>

<div class="container">
  <h1 class="label-default">Edit a question:</h1>

  <div class="container">
    <%= form_for @question, url: {id: @question.id, action: :update} do |f| %>
        <p>
          <%= f.label :position %><br>
          <%= f.text_field :position %>
        </p>

        <p>
          <%= f.label :summary %><br>
          <%= f.text_area :summary %>
        </p>

        <p>
          <%= f.label :question %><br>
          <%= f.text_area :question %>
        </p>
        <p>
          <%= f.label :max_rate %><br>
          <%= f.text_field :max_rate %>
        </p>

        <p>
          <%= f.submit %>
        </p>
    <% end %>
  </div>
</div>

