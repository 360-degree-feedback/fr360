
<h3> Question <%= f.object.question.title + ': ' %> </h3>
<div class="panel-heading"><h5><%= f.object.question.description %></h5></div>
<%= f.hidden_field :question_id, value: f.object.question.id %>
<%= render partial: 'comment_field' , locals: {f: f}%>
<%= render partial: 'radio_buttons', locals: {f: f, max: f.object.question.max_rate} %>